{
  "name": "express-boilerplate",
  "version": "1.0.0",
  "description": "boilerplate",
  "main": "server.js",
  "scripts": {
    "test": "node node_modules/.bin/istanbul cover --dir=documents/coverage --print=detail node_modules/.bin/_mocha -- -R spec --ui tdd --recursive tests",
    "no-cov-test": "node node_modules/.bin/mocha --reporter spec --ui tdd --recursive tests",
    "lint": "node node_modules/.bin/eslint -c .eslintrc --color --quiet --global __base --ignore-path .eslintignore \"backend/**\" \"frontend/**\" \"database/**\"",
    "analyze": "node node_modules/.bin/es6-plato -r -l .eslintrc -d documents/analysis backend",
    "docs": "node node_modules/.bin/jsdoc -r backend --readme README.md --destination documents/documentation --package package.json",
    "dev": "cross-env NODE_ENV=development webpack -d --progress --colors --watch --display-error-details",
    "build": "cross-env NODE_ENV=production webpack -p --progress --colors --display-chunks --hide-modules",
    "build-verbose": "cross-env NODE_ENV=production webpack -p --progress --colors --display-chunks",
    "dev-server": "cross-env NODE_ENV=development DEBUG=http nodejs-dashboard nodemon server.js",
    "nodemon": "cross-env NODE_ENV=development node_modules/.bin/nodemon server.js",
    "check": "npm-check",
    "check-update": "npm-check -u"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@belsrc/cryptkeeper": "1.0.1",
    "agenda": "1.0.0",
    "bluebird": "3.5.0",
    "body-parser": "1.17.2",
    "compression": "1.7.0",
    "connect-redis": "3.3.0",
    "connect-remote-ip": "0.2.2",
    "cookie-parser": "1.4.3",
    "di-con": "0.1.0",
    "dotenv": "4.0.0",
    "express": "4.15.4",
    "express-redirect-https": "0.1.0",
    "express-session": "1.15.5",
    "exreg-routes": "0.3.2",
    "headgear": "1.0.0",
    "mongo": "0.1.0",
    "mongodb": "2.2.31",
    "mongoose": "4.11.8",
    "morgan": "1.8.2",
    "nunjucks": "3.0.1",
    "on-headers": "1.0.1",
    "passport": "0.4.0",
    "passport-local": "1.0.0",
    "pg": "7.2.0",
    "pg-hstore": "2.3.2",
    "redis": "2.8.0",
    "sequelize": "4.7.5",
    "socket.io": "2.0.3",
    "useful-nunjucks-filters": "0.1.3",
    "winston": "2.3.1",
    "winston-daily-rotate-file": "1.4.6",
    "winston-mail": "1.3.0"
  },
  "devDependencies": {
    "axios": "0.16.2",
    "babel-core": "6.26.0",
    "babel-loader": "7.1.2",
    "babel-preset-latest": "6.24.1",
    "babel-preset-stage-1": "6.24.1",
    "babili": "0.1.4",
    "babili-webpack-plugin": "0.1.2",
    "chai": "4.1.1",
    "chai-as-promised": "7.1.1",
    "cross-env": "5.0.5",
    "css-loader": "0.28.5",
    "es6-plato": "1.0.14",
    "eslint": "4.5.0",
    "eslint-plugin-security": "1.4.0",
    "eslint-plugin-vue": "2.1.0",
    "extract-text-webpack-plugin": "3.0.0",
    "file-loader": "0.11.2",
    "istanbul": "0.4.5",
    "jsdoc": "3.5.4",
    "json-loader": "0.5.7",
    "mocha": "3.5.0",
    "node-sass": "4.5.3",
    "nodejs-dashboard": "0.4.1",
    "nodemon": "1.11.0",
    "sass-loader": "6.0.6",
    "sinon": "3.2.1",
    "socket.io-client": "2.0.3",
    "style-loader": "0.18.2",
    "vue": "2.4.2",
    "vue-loader": "13.0.4",
    "vue-router": "2.7.0",
    "vue-template-compiler": "2.4.2",
    "vuex": "2.3.1",
    "vuex-router-sync": "4.2.0",
    "webpack": "3.5.5"
  }
}
